{
  "trailingComma": "all",
  "singleQuote": false,
  "quoteProps": "consistent",
  "tabWidth": 2,
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-pkg"],
  "importOrder": [
    "",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^@/src/(.*)$",
    "^@/__tests__/(.*)$",
    "<THIRD_PARTY_TS_TYPES>",
    "<TS_TYPES>^[./]"
  ]
}

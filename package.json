{
	"name": "whatsapp",
	"version": "0.0.5-Alpha",
	"description": "SDK for interfacing with WhatsApp Business Platform in Typescript or Node.js using the Cloud API, hosted by Meta.",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		".": {
			"types": "./build/index.d.ts",
			"default": "./build/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"build",
		"src"
	],
	"type": "commonjs",
	"engines": {
		"node": ">=16.19.1"
	},
	"scripts": {
		"start": "ts-node -r tsconfig-paths/register ./src/WhatsApp.ts",
		"build": "rimraf ./build && tsc -p tsconfig.json && ./node_modules/.bin/babel build --out-dir build && rimraf ./build/__tests__/",
		"lint": "npm-run-all lint:*",
		"lint:eslint": "eslint .",
		"lint:cspell": "cspell \"{README.md,.github/*.md,docs/**/*.md}\"",
		"lint:typecheck": "tsc --noEmit",
		"test": "npm-run-all test:*",
		"test:jest": "jest",
		"format": "npm-run-all format:*",
		"format:prettier": "prettier . --write",
		"format-ci": "npm-run-all format-ci:*",
		"format-ci:prettier": "prettier . --check"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WhatsApp/WhatsApp-Nodejs-SDK.git"
	},
	"keywords": [
		"WhatsApp",
		"SDK",
		"Business",
		"Platform",
		"secure",
		"messaging",
		"NodeJS",
		"Cloud API"
	],
	"author": "Rashed Talukder",
	"contributors": [
		"Dom Webber <dom.webber@hotmail.com>",
		"Great Detail Ltd <info@greatdetail.com>"
	],
	"license": "SEE LICENSE IN LICENSE FILE",
	"bugs": {
		"url": "https://github.com/WhatsApp/WhatsApp-Nodejs-SDK/issues"
	},
	"homepage": "https://whatsapp.github.io/WhatsApp-Nodejs-SDK/",
	"devDependencies": {
		"@types/node": "~18.11.18",
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.4",
		"@babel/plugin-proposal-dynamic-import": "^7.18.6",
		"@babel/plugin-transform-modules-commonjs": "^7.21.2",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@jest/globals": "~29.5.0",
		"@types/jest": "^29.5.0",
		"@types/module-alias": "~2.0.1",
		"@typescript-eslint/eslint-plugin": "~5.57.1",
		"@typescript-eslint/parser": "~5.57.1",
		"babel-jest": "^29.5.0",
		"babel-plugin-add-module-exports": "^1.0.4",
		"cspell": "^6.31.1",
		"dotenv": "^16.0.3",
		"eslint": "~8.37.0",
		"eslint-config-prettier": "~8.8.0",
		"eslint-plugin-jest": "~27.2.1",
		"eslint-plugin-prettier": "~4.2.1",
		"graceful-fs": "~4.2.10",
		"isbinaryfile": "~5.0.0",
		"jest": "~29.5.0",
		"nock": "~13.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.7",
		"rimraf": "^4.4.1",
		"run-script-os": "^1.1.6",
		"ts-jest": "~29.1.0",
		"ts-node": "~10.9.1",
		"typescript": "~5.0.3"
	}
}
